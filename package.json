{
  "name": "stat-ee",
  "version": "1.0.0+6",
  "description": "",
  "main": "src/index.ts",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "scripts": {
    "start": "node dist/src/app.js",
    "test": "jest",
    "watch:build": "tsc --watch",
    "build": "tsc",
    "convert": "npm run build && node dist/src/converter.js",
    "ts-coverage": "typescript-coverage-report",
    "dev": "tsc && node dist/src/app.js",
    "docker": "npm run docker:build && npm run docker:save",
    "docker:build": "docker build -t stat-ee:latest .",
    "docker:save": "docker save -o stat-ee.tar stat-ee:latest",
    "docker:dev": "npm run docker:build && npm run docker:run",
    "docker:run": "docker run -d -p 3000:3000 -v $(pwd)/models:/home/appuser/stat-ee/models stat-ee:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crewnew/stat-ee.git"
  },
  "author": "Eusebiu A. Plesa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crewnew/stat-ee/issues"
  },
  "homepage": "https://github.com/crewnew/stat-ee#readme",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.20.0",
    "oracledb": "^6.5.1",
    "ts-jackson": "^1.6.0",
    "express": "^4.19.2",
    "dotenv": "^16.4.5",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
